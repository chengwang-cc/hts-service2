{
  "name": "hts-service-monorepo",
  "version": "0.0.1",
  "description": "HTS Service Monorepo - Multi-tenant HTS classification and calculation platform",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @hts/core build",
    "dev": "pnpm --filter hts-service dev",
    "format": "prettier --write \"**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,packages,apps}/**/*.ts\" --fix",
    "test": "pnpm -r test",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "migration:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./db/data-source.ts",
    "migration:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./db/data-source.ts",
    "migration:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./db/data-source.ts",
    "db:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/db/data-source.ts",
    "db:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "db:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/db/data-source.ts",
    "db:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/db/data-source.ts",
    "db:seed": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./src/seeds/seed.cli.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "export": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./src/db/data-migration.ts export",
    "import": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./src/db/data-migration.ts import",
    "cli": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./scripts/cli.ts",
    "cli:download-latest": "npm run cli download-latest",
    "cli:import-hts": "npm run cli import-hts",
    "cli:process-pdf": "npm run cli process-pdf",
    "cli:job-status": "npm run cli job-status",
    "cli:list-imports": "npm run cli list-imports",
    "cli:list-documents": "npm run cli list-documents",
    "cli:help": "npm run cli help"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/lib-storage": "^3.990.0",
    "@hts/billing": "workspace:*",
    "@hts/calculator": "workspace:*",
    "@hts/connectors": "workspace:*",
    "@hts/core": "workspace:*",
    "@hts/export": "workspace:*",
    "@hts/i18n": "workspace:*",
    "@hts/knowledgebase": "workspace:*",
    "@hts/lookup": "workspace:*",
    "@hts/onboarding": "workspace:*",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/typeorm": "^11.0.0",
    "@openai/agents": "^0.4.8",
    "axios": "^1.13.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "core": "^1.0.113",
    "dotenv": "^17.3.1",
    "image-size": "^2.0.2",
    "openai": "^4.104.0",
    "pg-boss": "^12.11.1",
    "puppeteer": "^24.37.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.28",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "bcryptjs": "^3.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
